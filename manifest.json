{
    "manifest_version": 3,
    "name": "Notes",
    "version": "1.0",
    "description": "Track time spent on different websites",
    "permissions": [
      "storage",
      "tabs",
      "activeTab",
      "webNavigation"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "index.html"
    }
  }